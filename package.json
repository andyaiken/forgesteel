		"axios-mock-adapter": "^2.1.0",
		"eslint": "^9.39.2",
		"eslint-plugin-react-hooks": "^7.0.1",
		"globals": "^17.0.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.51.0",
		"vite": "^7.3.0",
		"vite-tsconfig-paths": "^6.0.3",
		"vitest": "^4.0.16"
	}
