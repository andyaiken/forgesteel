		"axios": "^1.13.2",
		"gh-pages": "^6.3.0",
		"html2canvas": "^1.4.1",
		"jspdf": "^3.0.4",
		"localforage": "^1.10.0",
		"modern-screenshot": "^4.6.7",
		"pdf-lib": "^1.17.1",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"react-router": "^7.11.0",
		"sass": "^1.97.1",
		"showdown": "^2.1.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
