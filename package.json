{
	"name": "forgesteel",
	"private": true,
	"version": "11.80.0",
	"type": "module",
	"repository": "https://github.com/andyaiken/forgesteel.git",
	"homepage": "https://andyaiken.github.io/forgesteel/",
	"scripts": {
		"start": "vite --host",
		"update": "npx npm-check-updates --interactive --format group",
		"lint": "tsc -p tsconfig.node.json && tsc -p tsconfig.app.json && eslint . && npm audit",
		"fix": "eslint . --fix",
		"test": "vitest",
		"check": "npm run lint && vitest run",
		"build": "vite build",
		"predeploy": "npm run lint && vitest run && vite build",
		"deploy": "gh-pages -d dist"
	},
	"dependencies": {
		"@ant-design/icons": "^6.1.0",
		"@ant-design/v5-patch-for-react-19": "^1.0.3",
		"@types/showdown": "^2.0.6",
		"antd": "^5.27.6",
		"gh-pages": "^6.3.0",
		"html2canvas": "^1.4.1",
		"jspdf": "^3.0.3",
		"localforage": "^1.10.0",
		"modern-screenshot": "^4.6.6",
		"pdf-lib": "^1.17.1",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-router": "^7.9.4",
		"sass": "^1.93.2",
		"showdown": "^2.1.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.38.0",
		"@stylistic/eslint-plugin": "^5.5.0",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"@vitejs/plugin-react": "^5.0.4",
		"@vitest/coverage-v8": "^4.0.2",
		"eslint": "^9.38.0",
		"eslint-plugin-react-hooks": "^7.0.0",
		"globals": "^16.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.2",
		"vite": "^7.1.12",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^4.0.2"
	}
}
